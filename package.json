{
  "name": "@nandenjin/vue-md-editor",
  "version": "0.0.0",
  "main": "dist/md-editor.umd.js",
  "module": "dist/md-editor.esm.js",
  "unpkg": "dist/md-editor.min.js",
  "browser": {
    "./sfc": "src/md-editor.vue"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/my-component.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/my-component.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/my-component.min.js",
    "lint": "eslint --ext js,ts,vue --ignore-path .gitignore .",
    "serve": "vue serve --open src/dev.vue"
  },
  "repository": "https://github.com/nandenjin/md-editor.git",
  "author": "Kazumi Inada <hello@nandenjin.com>",
  "license": "MIT",
  "devDependencies": {
    "@vue/cli": "^3.11.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "rollup": "^1.21.2",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-vue": "^5.0.1",
    "sass-loader": "7.3.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
